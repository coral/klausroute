{
    "server": {
        "listen": "0.0.0.0",
        "port": 443,
        "persistence": true,
        "static_folder": "static",
        "bower": "bower_components",
        "ui": "semantic/dist",
        "ssl": {
                "enabled": true,
                "keyPath": "asterisk.pem",
                "certificatePath": "ca.crt"
        }
